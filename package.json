{
  "name": "openviber",
  "version": "0.5.2",
  "type": "module",
  "description": "OpenViber: You Imagine It. Vibers Build It.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openviber": "./dist/cli/index.js",
    "viber": "./dist/cli/index.js"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ],
    "overrides": {
      "zod": "^3.23.8",
      "ai": "^6.0.86"
    }
  },
  "scripts": {
    "dev": "tsx scripts/dev-setup.ts && concurrently -n gateway,web,viber \"tsx watch src/cli/index.ts gateway\" \"cd web && pnpm dev\" \"tsx watch src/cli/index.ts start --server ws://localhost:6009/ws\"",
    "dev:gateway": "tsx watch src/cli/index.ts gateway",
    "dev:standalone": "tsx watch src/cli/index.ts start",
    "dev:chat": "tsx src/cli/index.ts chat --gateway http://localhost:6009",
    "dev:web": "cd web && pnpm dev",
    "dev:setup": "tsx scripts/dev-setup.ts",
    "build": "tsup",
    "build:web": "cd web && pnpm build",
    "start": "node dist/cli/index.js start",
    "openviber": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "preview:web": "cd web && pnpm preview",
    "verify:skills": "tsx scripts/verify-skills.ts",
    "update:skills": "node --import tsx scripts/update-skills-catalog.ts",
    "verify:architecture": "tsx scripts/verify-architecture.ts",
    "docs:channels": "tsx scripts/generate-channel-capability-matrix.ts"
  },
  "keywords": [
    "multi-agent",
    "ai",
    "collaboration",
    "llm",
    "agent",
    "daemon"
  ],
  "author": "Dustland",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/deepseek": "^2.0.8",
    "@ai-sdk/openai": "^3.0.0",
    "@larksuiteoapi/node-sdk": "^1.58.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@openrouter/ai-sdk-provider": "^2.0.2",
    "@supabase/supabase-js": "^2.49.1",
    "ai": "^6.0.0",
    "commander": "^14.0.2",
    "croner": "^9.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "eventsource": "^4.1.0",
    "googleapis": "^171.4.0",
    "nanoid": "^3.3.8",
    "reflect-metadata": "^0.2.2",
    "telegraf": "^4.16.3",
    "undici": "^7.21.0",
    "ws": "^8.19.0",
    "yaml": "^2.8.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.0.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
